<div>
    <h1>New Heroine</h1>
</div>
<%= form_for @heroine do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name%>
    <br>
    <br>
    <%= f.label :super_name %>
    <%= f.text_field :super_name %>
    <br>
    <br>
    <%= f.submit %>
<% end %>
<br>
<br>

<%= link_to 'All Heroines', heroines_path%>