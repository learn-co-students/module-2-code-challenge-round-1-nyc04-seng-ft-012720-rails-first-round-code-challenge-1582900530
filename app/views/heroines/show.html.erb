<h1>
    <%= @heroine.super_name %>
</h1>

Secret Identity: <%= @heroine.name %>

<h3>
Powers
</h3>

    <ul>
        <% @heroine.powers.each{|power|%>
            <li>
                <%= link_to power.name.capitalize, power%>:
                <br>
                <%= power.description.capitalize %>
            </li>
        <%}%>
    </ul>