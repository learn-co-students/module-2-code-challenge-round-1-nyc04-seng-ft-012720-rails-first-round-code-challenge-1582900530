<h2>All Superpowers</h2>

<ul>
  <% @powers.each do |power| %>
    <li>
      <%= power.name.capitalize %>
      <br>
      Description: <%= power.description %>
    </li>
<h4>This Super Power is an Ability of the following SuperHeroines</h4>
     <% power.heroines.each do |heroine|%>
     <ul>
        <li>
           <%= link_to heroine.super_name.capitalize, heroine %>
        </li>
     </ul>
     <% end %>
     <br>
<% end %>
</ul>
